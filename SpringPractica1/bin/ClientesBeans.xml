<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:context ="http://www.springframework.org/schema/context"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://www.springframework.org/schema/context
http://www.springframework.org/schema/context/spring-context.xsd">
	
	<bean id="NeltzyCliente" class="com.springpractica1.beans.ClienteBean">
		<property name="idcliente" value="1"></property>
		<property name="nombreCliente" value="Neltzy"></property>
		<property name="domicilioCliente" value="10 Retas"></property>
	</bean>
	
	<bean id="GerardoCliente" class="com.springpractica1.beans.ClienteBean">
		<property name="idcliente" value="2"></property>
		<property name="nombreCliente" value="Gerardo"></property>
		<property name="domicilioCliente" value="5 Retas"></property>
	</bean>
	
	<bean id="DaniCliente" class="com.springpractica1.beans.ClienteBean">
		<property name="idcliente" value="3"></property>
		<property name="nombreCliente" value="Dani"></property>
		<property name="domicilioCliente" value="10 Retas"></property>
	</bean>
</beans>